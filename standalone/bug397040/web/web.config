<?xml version="1.0" encoding="ISO-8859-1" ?>
<configuration>
	<system.web>
		<compilation defaultLanguage="C#" debug="true" batch="false" />
		<customErrors mode="Off" />
		<trace enabled="false" localOnly="false" pageOutput="false" />
		<globalization fileEncoding="iso-8859-1" requestEncoding="iso-8859-1" responseEncoding="iso-8859-1" />
		<httpRuntime maxRequestLength="32768" />
		<sessionState timeout="60" />
		<siteMap>
			<providers>
				<add name="DynamicSiteMapProvider" type="DynamicSiteMapProvider" />
			</providers>
		</siteMap>
	</system.web>
</configuration>
